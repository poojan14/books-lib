<h2>Search Author</h2>
<form action="/authors" method="GET">
    <label for="name">Name:</label>
    <input type="text" name="name" value="<%= searchOptions.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% authors.forEach(function(author) { %>
    <div><%= author.name %></div>
    <%})%>